{
  "name": "tickie-contracts",
  "version": "1.0.0",
  "main": "hardhat.config.ts",
  "scripts": {
    "test": "npx hardhat test",
    "fork-deploy": "npx hardhat run --network hardhat scripts/deploy.ts",
    "mumbai-deploy": "npx hardhat run --network mumbai scripts/deploy.ts",
    "matic-deploy": "npx hardhat run --network polygon scripts/deploy.ts",
    "gen-exports": "npx ts-node scripts/gen/gen-exports.ts",
    "gen-fnsigs": "npx ts-node scripts/gen/gen-fnsigs.ts",
    "gen-interfaces": "npx ts-node scripts/gen/gen-interfaces.ts",
    "flat": "npx ts-node scripts/flatten.ts",
    "metadata": "npx ts-node scripts/metadata.ts",
    "format": "npx prettier --write . --config ./.prettierrc.yaml",
    "publish": "git branch -d main&git checkout --orphan main & git commit -m \"publish\" & git push -u client main -f & git checkout master",
    "postinstall": "hardhat typechain & npm run gen-interfaces"
  },
  "author": "bw",
  "license": "UNLICENCED",
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^2.0.2",
    "@openzeppelin/contracts": "^4.8.1",
    "@typechain/ethers-v5": "^10.2.0",
    "@typechain/hardhat": "^6.1.5",
    "@types/async": "^3.2.20",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^12.20.46",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "chai": "^4.3.7",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^5.2.0",
    "ethers": "^5.7.2",
    "hardhat": "^2.14.0",
    "jest": "^27.5.1",
    "mocha": "^10.2.0",
    "prettier": "^2.6.2",
    "prettier-plugin-solidity": "^1.0.0-beta.13",
    "solhint": "^3.3.7",
    "ts-node": "^10.5.0",
    "typechain": "^8.1.1",
    "typescript": "^4.5.5"
  }
